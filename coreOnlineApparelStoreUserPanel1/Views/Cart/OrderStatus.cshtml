    });

    $('#show-preview').click(function(e){
        e.preventDefault();

        markActive(this);

        $('.playground-editor').hide();

        var html = buildSource(htmleditor, jseditor, csseditor);
        var iframe = document.createElement('iframe');

        iframe.src = 'about:blank';
        iframe.frameBorder="0";
